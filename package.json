{
  "name": "kronos-step-archive-tar",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/tar.js",
  "module": "src/tar.js",
  "description": "step to tar/untar requests",
  "keywords": [
    "kronos-step"
  ],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "nyc --temp-directory build/nyc ava",
    "test": "ava",
    "semantic-release": "semantic-release",
    "precover": "rollup -c tests/rollup.config.js",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "docs": "documentation readme src/tar.js --section=API",
    "lint": "documentation lint src/tar.js",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "kronos-endpoint": "^2.14.16",
    "kronos-step": "^5.6.6",
    "tar-stream": "1.6.1"
  },
  "devDependencies": {
    "kronos-service-manager": "^3.5.10",
    "kronos-test-step": "^3.1.4",
    "semantic-release": "^15.5.0",
    "documentation": "^8.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^12.0.1",
    "ava": "^0.25.0",
    "rollup": "^0.59.4",
    "rollup-plugin-multi-entry": "^2.0.2",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": ">=10.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-step-archive-tar.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-step-archive-tar/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-step-archive-tar#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
