{
  "name": "kronos-step-archive-tar",
  "version": "0.0.0",
  "description": "step to tar/untar requests",
  "keywords": [
    "kronos-step"
  ],
  "main": "index.js",
  "scripts": {
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "doc": "./node_modules/.bin/jsdoc lib/*_test.js",
    "test": "mocha tests/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "contributors": [{
    "name": "Torsten Link",
    "email": "torstenlink@gmx.de"
  }, {
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "dependencies": {
    "kronos-step": "^3.9.0",
    "tar-stream": "1.3.1"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "chai-as-promised": "5.2.0",
    "cz-conventional-changelog": "^1.1.5",
    "istanbul": "0.4.2",
    "jsdoc": "3.4.0",
    "kronos-test-step": "^2.9.1",
    "mocha": "2.3.4",
    "semantic-release": "6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-step-archive-tar.git"
  },
  "engines": {
    "node": ">=4.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
